   nyukyoshazokugara=$(nameread NyukyoshaKazokuZokugara$knum $namedata | head -1 | join1 key=1 $tmp-zokugara_master | self 2)
   [ "$nyukyoshazokugara" = "_" ] && nyukyoshazokugara=""
