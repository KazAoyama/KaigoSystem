   [ "$nyukyoshakenkousonota" = "_" ] && nyukyoshakenkousonota=""
   echo -n '{"shimei":"'$nyukyoshashimei'","zokugara":"'$nyukyoshazokugara'","nenrei":"'$nyukyoshanenrei'","kenkou":"'$nyukyoshakenkou'","kenkousonota":"'$nyukyoshakenkousonota'"},' >> ${tmp}-kazokurecord
done

