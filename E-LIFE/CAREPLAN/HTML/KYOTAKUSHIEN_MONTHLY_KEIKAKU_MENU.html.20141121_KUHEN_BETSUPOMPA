//            $('.trTaishousha').eq(index).css('background-color','white');
//        });

        var realdate = new Date();
        var realseirekiyear = realdate.getFullYear();
        var firstseirekiyear = "2011";
        var curdate = new Date(###TAISHOUMONTH###);
        var seirekiyear = curdate.getFullYear();
        var nextmonth = curdate.getMonth()+parseInt(1);
        if(nextmonth>12){
            nextmonth=1;
        }
        for(var m=1; m<13; m++) {
            if(m==nextmonth) {
                valueM=('00'+m).slice(-2);
                $("#taishoumonth").append('<option value="'+valueM+'" selected>'+m+'</option>');
            } else {
                valueM=('00'+m).slice(-2);
                $("#taishoumonth").append('<option value="'+valueM+'">'+m+'</option>');
            }
        }

        for(var w in warekid) {
            $("#taishouwareki").append('<option value="'+warekid[w].id+'">'+warekid[w].value+'</option>');
        }

        var minusyear=0;
//  平成を選択
        $('#taishouwareki').val('05');
        for(var w in warekid) {
            if(warekid[w].id==$('#taishouwareki').val()) {
                minusyear=warekid[w].year;
                break;
            }
        }

        var curyear=seirekiyear-minusyear;
        var realyear=realseirekiyear-minusyear;
        var nextyear = realyear+parseInt(1);
        var firstyear = firstseirekiyear-minusyear;
        for (var y=firstyear;y<=realyear;y++) {
           $("#taishouyear").append('<option value="'+y+'">'+y+'</option>');
        };
        $("#taishouyear").append('<option value="'+nextyear+'">'+nextyear+'</option>');

        $("#taishouyear").val(curyear);

        function setSeirekiYear() {
            for(var w in warekid) {
                if(warekid[w].id==$('#taishouwareki').val()) {
                   plusyear=warekid[w].year;
                   break;
                }
            }
            var setYear=parseInt($('#taishouyear').val())+parseInt(plusyear);
            $('#hdTaishouYear').val(setYear);
        }

        $('#taishouyear').change(function(){
           setSeirekiYear();
        });

        $('#taishouwareki').change(function(){
           setSeirekiYear();
        });
        setSeirekiYear();

    $('#clear_btn').live('click',function(){
         $("#care_mane_kensaku").val('');
         $("#shisetsu_kensaku").val('');
         $(".kana_kensaku").val('');
         $(".btn.btn-default.active").removeClass("active");
     });
		$.datepicker.setDefaults({
			changeYear:true,
			changeMonth:true,
			yearRange:"c-130:c+1",
			dateFormat: 'yy/mm/dd',
	        dayNamesShort: ['日','月','火','水','木','金','土'],
            dayNamesMin: ['日','月','火','水','木','金','土'],
            yearSuffix: ''
        });
	});
  



        $(function(){
           $("#kensaku_date").datepicker({
              onSelect: function(dateText, inst){
                 var ArrDate=dateText.split("/");
                 var DispDate=convert_wareki(ArrDate[0],true)+"年"+parseInt(ArrDate[1])+"月"+parseInt(ArrDate[2])+"日";
                 $("#kensaku_date").val(DispDate);
                 $("#hdKensaku_date").val(ArrDate[0]+ArrDate[1]+ArrDate[2]);
              }
           });
        });


                function moveTopMenu() {
                    $('#MODE').val('init');
                    $('#hdkensakudate').val($('#hdKensaku_date').val());
                    document.frmScreenMenu.target="_self";
                    document.frmScreenMenu.action="KYOTAKUSHIEN_MENU_KAIGO.CGI";
                    document.frmScreenMenu.submit();
                }
		function convert_wareki(year, setting){
		    var result;
		    if(setting === false){
		        return year;
		    } else if (year > 1988) {
		        result = year - 1988;
		        result = '平成' + result;
		        return result;
		    } else if (year > 1925) {
		        result = year - 1925;
		        result = '昭和' + result;
		        return result;
		    } else if (year > 1911) {
		        result = y - 1911;
		        result = '大正' + result;
		        return result;
		    } else if (year > 1867) {
		        result = year - 1867;
		        result = '明治' + result;
		        return result;
		    }
		}


        function setRiyoushaid(id){
            $('#Riyoushaid').val(id);
        }

                $('.shisetsulist').live('mouseover',function(){
                   this.style.backgroundColor="lightgrey";
                });
                $('.shisetsulist').live('mouseout',function(){
                   this.style.backgroundColor="white";
                });


