    <script type="text/javascript">
        function Post(md,tx,shisetsu,url) {
            formlink.userid.value = md;
            formlink.password.value = tx;
            formlink.shisetsu.value = shisetsu;
            formlink.action=url;
            formlink.cgi.value = url;
            formlink.submit();
        }
    </script>


