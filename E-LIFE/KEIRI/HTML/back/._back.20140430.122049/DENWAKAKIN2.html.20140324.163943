<div class="header">
  <h1 class="page-title">電話課金取込</h1>
</div>
<div class="container-fluid">
  <div class="well">
    <div style="height:450px; overflow-y:auto">
      <div style="margin-top:1em;">
        <h3>課金データ取込</h3>
        <div class="btn-toolbar">
          <input class="btn btn-primary" id="phonedata_upload" type="button" value="課金データ取込" />
        </div>
        <div id="upload_open" style="display:none; margin-top:1em;">
          <div style="float:left;margin-right:1em;">
            <input class="btn btn-primary" id="upload_file" type="file" />
          </div>
          <div style="float:left;">
            <input class="btn btn-primary" id="file_upload" type="button" value="UPLOAD" />
          </div>
        </div>
        <div style="margin-top:2em;">
          <h3>帳票出力</h3>
        </div>
        <div style="float:left">
          <table style="margin-left:2em;min-width:1024px;">
            <tr>
              <td style="width:10%"><input name="nmRiyoushamei" style="vertical-align:center;" type="radio" />全選択
              </td>
              <td style=""><input id="SlRiyoushamei" name="nmRiyoushamei" style="vertical-align:center;" type="radio" />利用者選択
                <input id="Riyoushamei" name="Riyoushamei" style="width:150px;vertical-align:center;" type="text" />
              </td>
            </tr>
          </table>
        </div>
        <div style="float:left;margin-top:1em">
          <table style="margin-left:2em;min-width:1024px;">
            <tr>
              <td style="width:15%">
                <select id="nmChouhyou" style="vertical-align:center;">
                  <option value="1">電話料金利用明細</option>
                  <option value="2">電話課金集計表</option>
                </select>
              </td>
              <td align="left" style="width:10%">
                <select id="nmPeriod" style="vertical-align:center;">
                  <option value="1">月次</option>
                  <option disabled="" value="2">日次</option>
                </select>
              </td>
              <td align="left" style=""><select id="KikanYear" name="KikanYear"></select>年<select id="KikanMonth" name="KikanMonth"></select>月<select disabled="" id="KikanDay" name="KikanDay"></select>日
              </td>
            </tr>
          </table>
        </div>
        <div class="clearfix"></div>
        <div class="btn-toolbar" style="margin-top:6em">
          <button class="btn btn-primary" id="print"><i class="icon-print"></i>Excel出力
          </button>
        </div>
      </div>
    </div>
  </div>
</div>

    <script src="/js/bootstrap.js"></script>
    <script type="text/javascript">
        $("[rel=tooltip]").tooltip();
        $(function() {
            $('.demo-cancel-click').click(function(){return false;});
        });
    </script>
    
  </body>
</html>


        
