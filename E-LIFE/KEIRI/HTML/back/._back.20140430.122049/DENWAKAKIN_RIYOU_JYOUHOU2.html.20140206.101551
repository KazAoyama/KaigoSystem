    <!-- formここから -->
    <form method="post" name="input_form" id="input_form">
    <input type="hidden" id="MODE" name="MODE"/> <!--mode設定-->
    <input type="hidden" id="userid" name="userid" value="###USERID###" /> <!--作業者ID-->
    <input type="hidden" id="password" name="password" value="###PASSWORD###" /> <!--pass-->
    <input type="hidden" id="shisetsu" name="shisetsu" value="###SHISETSUID###" /> <!--作業者施設-->
    <input type="hidden" id="shisetsu_key" name="shisetsu_key" value="###shisetsu_key###" /> <!--施設キー-->
    <input type="hidden" id="ExcelPass" name="ExcelPass" /> <!--excel出力時の出力ファイル名吐き出し先-->
    <div class="header">
      <h1 class="page-title">電話課金取込</h1>
    </div>

    <div class="row-fluid">
      <div class="display_1" style="display:###div_display_1###;">
        <div style="margin-left:1em; margin-top:1em;"><h4>課金データ取込</h4></div>
        <div style="margin-left:2em; margin-top:1em; margin-bottom:1em; float:left;"><input class="" id="upload_file" type="file" /></div>
        <div style="margin-left:1em; margin-top:1em; margin-bottom:1em; float:left;">
          <input class="btn btn-primary" id="file_upload" value="アップロード" type="button" style="margin-right:1em;" />
          <a href="javascript:void(0);" class="jump_page" id="to_syuturyoku" name="to_syuturyoku">＞＞帳票出力へ</a>
        </div>
      </div>
      <div class="display_2" style="display:###div_display_2###;">
        <div style="margin-left:1em; margin-top:1em; margin-bottom:1em; clear:both;"><h4>帳票出力</h4></div>
        <table style="margin-left:2em; margin-top:1em; margin-bottom:1em; float:left;">
          <tr>
            <td style="text-align:right;">施設名：</td>
            <td style="padding:0px 20px 0px 0px;">
              <select id="Shisetsu_select" name="Shisetsu_select" style="width:120px;" readonly>
                <!-- shisetsu_tbl -->
                <option value="%1" %3>%2</option>
                <!-- shisetsu_tbl -->
              </select>
            </td>
            <td style="text-align:right;">建屋：</td>
            <td style="padding:0px 20px 0px 0px;">
              <select id="Tateya_select" name="Tateya_select" style="width:120px;" onchange="javascript:ajax_change('tateya');">
                <!-- tateya_tbl -->
                <option value="%1" %3>%2</option>
                <!-- tateya_tbl -->
              </select>
            </td>
            <td colspan="4" id="ajax_change_td" name="ajax_change_td">
              <!-- ajax_change_start -->
              <table>
                <tr>
                  <td style="text-align:right;">フロア：</td>
                  <td style="padding:0px 20px 0px 0px;">
                    <select id="Floor_select" name="Floor_select" style="width:120px;" onchange="javascript:ajax_change('floor');">
                      <!-- floor_tbl -->
                      <option value="%1" %3>%2</option>
                      <!-- floor_tbl -->
                    </select>
                  </td>
                  <td class="unit_td" style="text-align:right; visibility:###visivility1###">ユニット：</td>
                  <td class="unit_td" style="padding:0px 20px 0px 0px; visibility:###visivility2###;">
                    <select id="Unit_select" name="Unit_select" style="width:120px;">
                      <!-- unit_tbl -->
                      <option value="%1" %3>%2</option>
                      <!-- unit_tbl -->
                    </select>
                  </td>
                </tr>
              </table>
              <!-- ajax_change_end -->
            </td>
          </tr>
          <tr><td colspan="8" style="height:10px;"></td></tr>
          <tr>
            <td style="text-align:right;">帳票種別：</td>
            <td style="padding:0px 20px 0px 0px;">
              <select style="width:120px;" id="syubetsu_select" name="syubetsu_select">
                <!-- syubetsu_tbl -->
                <option value="%1" %3>%2</option>
                <!-- syubetsu_tbl -->
              </select>
            </td>
            <td colspan="6" id="ajax_change2_td" name="ajax_change2_td">
              <!-- ajax_change2_start -->
              <table>
                <tr>
                  <td style="text-align:right;">期間：</td>
                  <td style="padding:0px 20px 0px 0px;">
                    <select style="width:120px;" id="kikan_select" name="kikan_select">
                      <!-- kikan_syubetsu_tbl -->
                      <option value="%1" %3>%2</option>
                      <!-- kikan_syubetsu_tbl -->
                    </select>
                  </td>
                  <td colspan="4" id="kikan_from_to" name="kikan_from_to" style="display:###kikan_display1###;">
                    <table>
                      <tr>
                        <td style="padding:0px 0px 0px 0px;"><input type="text" id="kikan_from" name="kikan_from" style="width:100px;" value="" /></td><!--期間FROM-->
                        <td style="padding:0px 5px 0px 5px;">～</td>
                        <td style="padding:0px 0px 0px 0px;"><input type="text" id="kikan_to" name="kikan_to" style="width:100px;" value="" /></td><!--期間TO-->
                      </tr>
                    </table>
                  </td>
                  <td colspan="4" id="kikan_month" name="kikan_month" style="display:###kikan_display2###;">
                    <table>
                      <tr>
                        <td>
                          <select>
                            <!-- kikan_month_tbl -->
                            <option value="%1" %3>%2</option>
                            <!-- kikan_month_tbl -->
                          </select>
                        <td>
                      </tr>
                    </table>
                  </td>
                </tr>
              </table>
              <!-- ajax_change2_end -->
            </td>
          </tr>
        </table>
        <div style="float:left" class="btn-toolbar" style="margin-top:6em;">
          <button type="button" class="btn btn-primary" id="search" style="margin-left:1em; margin-top:1em; margin-bottom:1em;"><i class="icon-search"></i>検索</button>
          <button type="button" class="btn btn-primary" id="print" style="margin-top:1em; margin-bottom:1em; margin-right:1em;"><i class="icon-print"></i>帳票出力</button>
          <a href="javascript:void(0);" class="jump_page" id="to_torikomi" name="to_torikomi">＞＞課金データ取り込みへ</a>
        </div>
      </div>
    </div>

    <div class="container-fluid">
      <div class="well">
        <div style="height:450px; overflow-y:auto">
          <div style="margin-top:1em;">
            <table class="check_list_table display_2" style="margin-left:20px; display:###check_list_display###;">
              <thead>
                <tr>
                  <th class="btn-success" style="text-align:center; width:50px;"><font style="font-size:12px;">全選択</font></br><input type="checkbox" id="check_all" name="check_all" /></th>
                  <th class="btn-success" style="text-align:center; width:200px;">入居者ID</th>
                  <th class="btn-success" style="text-align:center; width:200px;">入居者名</th>
                </tr>
              </thead>
              <tbody>
              <!-- Check_list_table -->
              <tr>
                <td class="check_list_td"><input type="checkbox" id="check_person" name="check_person" value="%1" class="check_person" /></td>
                <td class="check_list_td">%1</td>
                <td class="check_list_td">%2</td>
              </tr>
              <!-- Check_list_table -->
            </tbody>
            </table>
          </div>
       </div>
    </div>

    </form>
    <!-- formここまで -->

    <script src="/js/bootstrap.js"></script>
    <script type="text/javascript">
        $("[rel=tooltip]").tooltip();
        $(function() {
            $('.demo-cancel-click').click(function(){return false;});
        });
    </script>
    
  </body>
</html>


        
