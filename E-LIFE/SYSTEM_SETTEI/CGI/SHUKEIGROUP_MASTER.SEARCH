#!/bin/bash
#
# SHUKEIGROUP_MASTER.DISPLAY
# 集計グループマスタ　表示用CGI
#
# Written by K.Aoyama :20131228

#--------------------------------------------------------------
# ログ
source /home/hands/E-LIFE/SYSTEM_SETTEI/CGI/SYSTEM_SETTEI.INI &> /dev/null
source /home/hands/.bashrc &> /dev/null
mkdir -p ${log_dir}/$(date +%Y%m%d)
exec 2> ${log_dir}/$(date +%Y%m%d)/LOG.$(basename ${0}).$HOSTNAME.$(date +%Y%m%d) ; set -xv
#--------------------------------------------------------------

#--------------------------------------------------------------
rm -f  $tmp-*
#--------------------------------------------------------------

#--------------------------------------------------------------
function error_exit {
  rm -f $tmp-*
  exit 0
}
#--------------------------------------------------------------

#--------------------------------------------------------------
# 変数の定義
namedata=$1

# 必要な値の取得
eval $(name-source ${namedata})
#--------------------------------------------------------------

# テスト

#--------------------------------------------------------------
# 入力データのチェック
# ユーザＩＤ
[ -z "${userid}" -o "${userid}" = "_" ] && error_exit "ログインユーザが不明です"

#--------------------------------------------------------------
# ヘッダー部分生成
#--------------------------------------------------------------
cat <<- FIN > $tmp-header
FIN

#--------------------------------------------------------------
# ファイルの取得
cat ${home_dir}/TBL/SEIKYU/SHUKEIGROUP_MASTER |
# 1:集計グループID 2:集計グループ
awk '$3=="'${shisetsu}'" && $(NF-2)!=9'              |
#--------------------------------------------------------------
delf NF-2/NF  > ${tmp}-shukeigroup_master
[ $(plus ${PIPESTATUS[@]}) -ne 0 ] && error_exit "集計グループマスタ取得エラー"


#--------------------------------------------------------------
# 出力
cat ${tmp}-shukeigroup_master


## 終了
rm -f $tmp-*
exit 0
