join0 key=1 $tmp-shisetsu                                    |
if [ "$dispTaikyo" = "1" ] ; then
  cat -
else
  awk '$8>="'${today}'"'                   
fi > $tmp-riyosha_status



[ -z "${MODE}" ] && MODE="init"
#--------------------------------------------------------------

if [ "$MODE" = "init" ];then
