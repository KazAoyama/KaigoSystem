eval $(name-source ${namedata})
#--------------------------------------------------------

# shisetsu_tateya_floor_roomのマスタ準備
echo $shisetsu > $tmp-shisetsu
cat ${home_dir}/TBL/SHISETSU_MASTER/SHISETSU_TATEYA_FLOOR_UNIT_ROOM               |
