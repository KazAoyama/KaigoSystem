cat $tmp-taikyosha_hyouji_before2
else
cat $tmp-taikyosha_hyouji_before 
fi |
awk '{if($1=="'${TaikyoshaHyouji}'"){print $0,"selected"}
      else{print $0,"@"}
     }'                          > $tmp-taikyosha_hyouji


