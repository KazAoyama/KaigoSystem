#awk '{if($5=="1"){print $1,$2,$3,$4,"男"}
#     else if($5=="2"){print $1,$2,$3,$4,"女"}
#     }' |
# 1:居室番号      2:利用者(入居者)ID 3:利用者(入居者)氏名 4:利用者(入居者)氏名カナ 5:性別

# 表示用ソートはDISPLAYにて！！
#--------------------------------------------------------------
#cp $tmp-* /home/hands/work/okuda

cat $tmp-result_data


