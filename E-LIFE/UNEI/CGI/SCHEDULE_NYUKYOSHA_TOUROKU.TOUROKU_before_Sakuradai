            if [ -n "${GuestCateringNum4}" -a "${GuestCateringNum4}" != "_" ] ; then
               if [ "$catering_shokuji_jisseki_id42" = "_" ] ; then
                 shokuji_kanri_dir="/DATA/E-LIFE/SHOKUJI/KANRI"
