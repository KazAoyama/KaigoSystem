	$(document).ready(function() {

        $("#touroku").click(function(){
            $("#PopKeiyaku [name=MODE]").val("touroku");
            document.PopKeiyaku.action="POP_KOJINJYOUHOU_SHOUSAI_KEIYAKU.CGI";
            document.PopKeiyaku.submit();
        });

        $("#KyoshitsuHenkou").click(function(){
           if(!confirm("施設・建屋・フロア・ユニット・居室情報を変更しますか？")){
              return false;
             }
            $("#Shisetsu").removeAttr("disabled");
            $("#Tateya").removeAttr("disabled"); 
            $("#Floor").removeAttr("disabled");
            $("#Unit").removeAttr("disabled");
            $("#Kyoshitsu").removeAttr("disabled");
            $("#KeiyakuNenrei").attr('readonly',false);
        });
		$.datepicker.setDefaults({
			changeYear:true,
			changeMonth:true,
			yearRange:"c-10:c+10",
			dateFormat: 'yy/mm/dd',
			yearSuffix: ''
        });



		$(function(){
			$("#Keiyakubi").datepicker();
