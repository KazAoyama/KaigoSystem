        var keiyakukinhtml="";
        var keiyakukinnum=0;

       function setKeiyakuHTMLFormat() {
            keiyakukinhtml="";
            keiyakukinhtml=keiyakukinhtml+'<tr>';
            keiyakukinhtml=keiyakukinhtml+'<td style="text-align:center; border:#bbbbbb solid 1px;">';
            keiyakukinhtml=keiyakukinhtml+'<select id="KeiyakukinShubetsu'+keiyakukinnum+'" name="KeiyakukinShubetsu'+keiyakukinnum+'" class="KeiyakukinShubetsu" style="width:100%"></select>';
            keiyakukinhtml=keiyakukinhtml+'</td>';
            keiyakukinhtml=keiyakukinhtml+'<td style="text-align:center; border:#bbbbbb solid 1px;">';
            keiyakukinhtml=keiyakukinhtml+'<input name="KeiyakukinShoukyakubiFrom'+keiyakukinnum+'" id="KeiyakukinShoukyakubiFrom'+keiyakukinnum+'" style="width:100%; background-color:lightyellow;" type="text" class="KeiyakukinShoukyakubiFrom">';
            keiyakukinhtml=keiyakukinhtml+'</td>';
            keiyakukinhtml=keiyakukinhtml+'<td style="text-align:center; border:#bbbbbb solid 1px;">';
            keiyakukinhtml=keiyakukinhtml+'<input name="KeiyakukinShoukyakubiTo'+keiyakukinnum+'" id="KeiyakukinShoukyakubiTo'+keiyakukinnum+'" style="width:100%; background-color:lightyellow;" type="text" class="KeiyakukinShoukyakubiTo">';
            keiyakukinhtml=keiyakukinhtml+'</td>';
            keiyakukinhtml=keiyakukinhtml+'<td style="text-align:center; border:#bbbbbb solid 1px;">';
            keiyakukinhtml=keiyakukinhtml+'<input type="text" id="KeiyakukinKingaku'+keiyakukinnum+'" name="KeiyakukinKingaku'+keiyakukinnum+'" style="width:100%; text-align:right;">';
            keiyakukinhtml=keiyakukinhtml+'</td>';
            keiyakukinhtml=keiyakukinhtml+'<td style="text-align:center; border:#bbbbbb solid 1px;">';
            keiyakukinhtml=keiyakukinhtml+'<input type="text" id="KeiyakukinShoukyakuKaisuu'+keiyakukinnum+'" name="KeiyakukinShoukyakuKaisuu'+keiyakukinnum+'" style="width:100%; text-align:right;">';
            keiyakukinhtml=keiyakukinhtml+'</td>';
            keiyakukinhtml=keiyakukinhtml+'<td style="text-align:center; border:#bbbbbb solid 1px;">';
            keiyakukinhtml=keiyakukinhtml+'<input type="text" id="KeiyakukinHihenkyakuKingaku'+keiyakukinnum+'" name="KeiyakukinHihenkyakuKingaku'+keiyakukinnum+'" style="width:100%; text-align:right;">';
            keiyakukinhtml=keiyakukinhtml+'</td>';
            keiyakukinhtml=keiyakukinhtml+'<td style="text-align:center; border:#bbbbbb solid 1px;">';
            keiyakukinhtml=keiyakukinhtml+'<select id="KeiyakukinSanshutsuHouhou'+keiyakukinnum+'" name="KeiyakukinSanshutsuHouhou'+keiyakukinnum+'" style="width:100%">';
            keiyakukinhtml=keiyakukinhtml+'<option style="width:100%" value="1">日割';
            keiyakukinhtml=keiyakukinhtml+'</option>';
            keiyakukinhtml=keiyakukinhtml+'<option style="width:100%" value="2">月割';
            keiyakukinhtml=keiyakukinhtml+'</option>';
            keiyakukinhtml=keiyakukinhtml+'</select>';
            keiyakukinhtml=keiyakukinhtml+'</td>';
            keiyakukinhtml=keiyakukinhtml+'<td style="text-align:center; border:#bbbbbb solid 1px;">';
            keiyakukinhtml=keiyakukinhtml+'<select id="KeiyakukinTokuhanShubetsu'+keiyakukinnum+'" name="KeiyakukinTokuhanShubetsu'+keiyakukinnum+'" class="KeiyakukinTokuhanShubetsu" style="width:100%"></select>';
            keiyakukinhtml=keiyakukinhtml+'</td>';
            keiyakukinhtml=keiyakukinhtml+'<td style="text-align:center; border:#bbbbbb solid 1px;">';
            keiyakukinhtml=keiyakukinhtml+'<input type="text" id="KeiyakukinNebikiKingaku'+keiyakukinnum+'" name="KeiyakukinNebikiKingaku'+keiyakukinnum+'" style="width:100%; text-align:right;">';
            keiyakukinhtml=keiyakukinhtml+'</td>';
            keiyakukinhtml=keiyakukinhtml+'<td style="text-align:center; border:#bbbbbb solid 1px;">';
            keiyakukinhtml=keiyakukinhtml+'<input type="text" id="KeiyakukinNebikiGoKingaku'+keiyakukinnum+'" name="KeiyakukinNebikiGoKingaku'+keiyakukinnum+'" style="width:100%; text-align:right;">';
            keiyakukinhtml=keiyakukinhtml+'</td>';
            keiyakukinhtml=keiyakukinhtml+'<td style="text-align:center; border:#bbbbbb solid 1px;">';
            keiyakukinhtml=keiyakukinhtml+'<input name="KeiyakukinTokuhanFrom'+keiyakukinnum+'" id="KeiyakukinTokuhanFrom'+keiyakukinnum+'" style="width:100%; background-color:lightyellow;" type="text" class="KeiyakukinShoukyakubiFrom">';
            keiyakukinhtml=keiyakukinhtml+'</td>';
            keiyakukinhtml=keiyakukinhtml+'<td style="text-align:center; border:#bbbbbb solid 1px;">';
            keiyakukinhtml=keiyakukinhtml+'<input name="KeiyakukinTokuhanTo'+keiyakukinnum+'" id="KeiyakukinTokuhanTo'+keiyakukinnum+'" style="width:100%; background-color:lightyellow;" type="text" class="KeiyakukinShoukyakubiTo">';
            keiyakukinhtml=keiyakukinhtml+'</td>';
            keiyakukinhtml=keiyakukinhtml+'</tr>';

            $('#KeiyakukinIchiran').append(keiyakukinhtml);
            setKeiyakukinShubetsu(keiyakukinnum);    
			$(".KeiyakukinShoukyakubiFrom").datepicker();
			$(".KeiyakukinShoukyakubiTo").datepicker();
            keiyakukinnum++;

        }


        if(keiyakukind.length>0) {
            for(var sk in keiyakukind) {
                setKeiyakuHTMLFormat();
                $('#KeiyakukinShubetsu'+sk).val(keiyakukind[sk].shubetsuid);
                $('#KeiyakukinShoukyakubiFrom'+sk).val(keiyakukind[sk].syoukyakubifrom);
                $('#KeiyakukinShoukyakubiTo'+sk).val(keiyakukind[sk].shoukyakubito);
                $('#KeiyakukinKingaku'+sk).val(keiyakukind[sk].kingaku);
                $('#KeiyakukinShoukyakuKaisuu'+sk).val(keiyakukind[sk].shoukyakukaisuu);
                $('#KeiyakukinHihenkyakuKingaku'+sk).val(keiyakukind[sk].hihenkyakkukin);
                $('#KeiyakukinSanshutsuHouhou'+sk).val(keiyakukind[sk].sanshutsuhouhou);
                $('#KeiyakukinTokuhanShubetsu'+sk).val(keiyakukind[sk].tokuhanid);
                $('#KeiyakukinNebikiKingaku'+sk).val(keiyakukind[sk].nebikikingaku);
                $('#KeiyakukinNebikiGoKingaku'+sk).val(keiyakukind[sk].nebikigokingaku);
                $('#KeiyakukinTokuhanFrom'+sk).val(keiyakukind[sk].tokuhanbifrom);
                $('#KeiyakukinTokuhanTo'+sk).val(keiyakukind[sk].tokuhanbito);
            }
        } else {
            setKeiyakuHTMLFormat();
        }
            jQuery('#keiyakukin_add').click(function(){
                setKeiyakuHTMLFormat();
            });




