<!--

function shisetsu_keiyaku(str){
   if(str.match(/^0[0-9]/)) {
      document.getElementById('KEIYAKUDISP').style.display = "";
      document.getElementById('KEIYAKUDISP2').style.display = "";

  }else{
      document.getElementById('KEIYAKUDISP').style.display = "none";
      document.getElementById('KEIYAKUDISP2').style.display = "none";
}

}

function keiyaku_houkatsu(str){
   if(str.match("21")) {
      document.getElementById('HOUKATSU').style.display = "";
   }else if(str.match("27")) {
      document.getElementById('HOUKATSU').style.display = "";
   }else{
      document.getElementById('HOUKATSU').style.display = "none";
}

}

