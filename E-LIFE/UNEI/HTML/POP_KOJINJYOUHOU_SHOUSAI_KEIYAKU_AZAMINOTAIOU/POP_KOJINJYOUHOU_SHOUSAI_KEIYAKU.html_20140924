//特販追加

        var tokuhanhtml="";
        var tokuhannum=0;

       function setTokuhanHTMLFormat() {
            tokuhanhtml="";
            tokuhanhtml=tokuhanhtml+'<tr>';
            tokuhanhtml=tokuhanhtml+'<td style="text-align:center; border:#bbbbbb solid 1px;">';
            tokuhanhtml=tokuhanhtml+'<select id="TokuhanShubetsu'+tokuhannum+'" name="TokuhanShubetsu'+tokuhannum+'" class="TokuhanShubetsu" style="width:100%; text-align:right;"></select>';
            tokuhanhtml=tokuhanhtml+'</td>';
            tokuhanhtml=tokuhanhtml+'<td style="text-align:center; border:#bbbbbb solid 1px;">';
            tokuhanhtml=tokuhanhtml+'<input type="text" id="TokuhanNebikiKingaku'+tokuhannum+'" name="TokuhanNebikiKingaku'+tokuhannum+'" style="width:100%; text-align:right;">';
            tokuhanhtml=tokuhanhtml+'</td>';
            tokuhanhtml=tokuhanhtml+'<td style="text-align:center; border:#bbbbbb solid 1px;">';
            tokuhanhtml=tokuhanhtml+'<input name="TokuhanbiFrom'+tokuhannum+'" style="width:100%; background-color:lightyellow;" type="text" class="TokuhanbiFrom" id="TokuhanbiFrom'+tokuhannum+'">';
            tokuhanhtml=tokuhanhtml+'</td>';
            tokuhanhtml=tokuhanhtml+'<td style="text-align:center; border:#bbbbbb solid 1px;">';
            tokuhanhtml=tokuhanhtml+'<input name="TokuhanbiTo'+tokuhannum+'" style="width:100%; background-color:lightyellow;" type="text" class="TokuhanbiTo" id="TokuhanbiTo'+tokuhannum+'">';
            tokuhanhtml=tokuhanhtml+'</td>';
            tokuhanhtml=tokuhanhtml+'<td style="text-align:center; border:#bbbbbb solid 1px;">';
            tokuhanhtml=tokuhanhtml+'<input type="text" id="TokuhanNaiyou'+tokuhannum+'" name="TokuhanNaiyou'+tokuhannum+'" style="width:100%; text-align:right;">';
            tokuhanhtml=tokuhanhtml+'</td>';
            tokuhanhtml=tokuhanhtml+'</tr>';

            $('#TokuhanIchiran').append(tokuhanhtml);
            setTokuhanShubetsu(tokuhannum);    
			$(".TokuhanbiFrom").datepicker();
			$(".TokuhanbiTo").datepicker();
            tokuhannum++;
        }

        if(tokuhand.length>0) {
            for(var sk in tokuhand) {
                setTokuhanHTMLFormat();
                $('#TokuhanShubetsu'+sk).val(tokuhand[sk].shubetsuid);
                $('#TokuhanNebikiKingaku'+sk).val(tokuhand[sk].nebikikingaku);
                $('#TokuhanbiFrom'+sk).val(tokuhand[sk].tokuhanbifrom);
                $('#TokuhanbiTo'+sk).val(tokuhand[sk].tokuhanbito);
                $('#TokuhanNaiyou'+sk).val(tokuhand[sk].naiyou);
            }
        } else {
            setTokuhanHTMLFormat();
        }
            jQuery('#tokuhan_add').click(function(){
                setTokuhanHTMLFormat();
            });





     $(document).on("change" ,"#Shisetsu" ,function(){
        var data = { GAMEN : "KEIYAKU"  , MODE : "Shisetsu" ,Shisetsu : $("#Shisetsu").val(),KeiyakuNenrei : $("#KeiyakuNenrei").val(),KeiyakuShurui : $("#KeiyakuShurui").val() , RIYOUSHAID : $("#RIYOUSHAID").val()};
       $.ajax({
             url:"AJAX_KOJINJYOUHOU_SHINKI.CGI",
             type:"POST",
             data:data,
             success:function(response){
              $("#ShisetsuSelectDiv").html(response);
          }
       })
       $.ajax({
             url:"AJAX_KOJINJYOUHOU_SHINKI_KEIYAKU.CGI",
             type:"POST",
             data:data,
             success:function(response){
              $("#KeiyakuShuruiDiv").html(response);
          }
       })
       $.ajax({
             url:"AJAX_KOJINJYOUHOU_SHINKI_KEIYAKU_DATA.CGI",
             type:"POST",
             data:data,
             success:function(response){
              $("#KeiyakuData").html(response);
          }

       })

       $.ajax({
             url:"AJAX_KOJINJYOUHOU_SHINKI_RIYOURYOU.CGI",
             type:"POST",
             data:data,
             success:function(response){
              $("#RiyouryouTbl").html(response);
              setKeiyakukinShubetsu(0);
              setRiyouryouShubetsu(0);
              setTokuhanShubetsu(0);

            jQuery('#keiyakukin_add').click(function(){
                setKeiyakuHTMLFormat();
            });
            jQuery('#riyouryou_add').click(function(){
                setRiyouryouHTMLFormat();
            });
            jQuery('#tokuhan_add').click(function(){
                setTokuhanHTMLFormat();
            });

          }
       })

    });


     $(document).on("change" , "#Tateya" ,function(){
        var data = { GAMEN : "KEIYAKU"  , MODE : "Tateya" ,RIYOUSHAID : $("#RIYOUSHAID").val(),KeiyakuShurui : $("#KeiyakuShurui").val(),Shisetsu : $("#Shisetsu").val(), Tateya : $("#Tateya").val(),KeiyakuNenrei : $("#KeiyakuNenrei").val() , RIYOUSHAID : $("#RIYOUSHAID").val() ,  KeiyakukikanFrom : $("#KeiyakukikanFrom").val()};
       $.ajax({
             url:"AJAX_KOJINJYOUHOU_SHINKI.CGI",
             type:"POST",
             data:data,
             success:function(response){
              $("#ShisetsuSelectDiv").html(response);
// 月額賃料の変更
              alert("月額賃料を変更した居室の料金へ更新します。");
              $(".RiyouryouShubetsu").each( function() {
                var val= $(this).val();
                var index = $(".RiyouryouShubetsu").index(this);
                for (var R in riyouryou_kingaku_master) {
                var riyouryou=riyouryou_kingaku_master[R].id;
              if(val=="001") {
              $(".RiyouryouKingaku").eq(index).val($('#CHINRYOU').val());
//              $(".RiyouryouTokuhanShubetsu").eq(index).val("");
              $(".RiyouryouNebikiKingaku").eq(index).val("");
              $(".RiyouryouNebikiGoKingaku").eq(index).val("");
//              $(".RiyouryouTokuhanFrom").eq(index).val("");
//              $(".RiyouryouTokuhanTo").eq(index).val("");
              }else{
              if(val==riyouryou) {
                 $(".RiyouryouKingaku").eq(index).val(riyouryou_kingaku_master[R].value);
                   break;
                                  }
                   }
              }
             })
          }
       })

    });

