                        $(".TokuhanbiTo").datepicker();
                    });


		function convert_wareki(year, setting){
		    var result;
		    if(setting === false){
		        return year;
		    } else if (year > 1988) {
		        result = year - 1988;
		        result = '平成 ' + result;
		        return result;
		    } else if (year > 1925) {
		        result = year - 1925;
		        result = '昭和 ' + result;
		        return result;
		    } else if (year > 1911) {
		        result = y - 1911;
		        result = '大正 ' + result;
		        return result;
		    } else if (year > 1867) {
		        result = year - 1867;
		        result = '明治 ' + result;
		        return result;
		    }
		}

//契約金マスタの設置

      function setKeiyakukinShubetsu(num) {    
	      if(keiyakukin_master.length>0) {
	         for(var zo in keiyakukin_master) {
//	            $('.KeiyakukinShubetsu').eq(num).append('<option value="'+keiyakukin_master[zo].id+'">'+keiyakukin_master[zo].value+'</option>');
	            $('#KeiyakukinShubetsu'+num).append('<option value="'+keiyakukin_master[zo].id+'">'+keiyakukin_master[zo].value+'</option>');
      }
              }
              if(keiyakukin_tokuhan_master.length>0) {
                 for(var zo in keiyakukin_tokuhan_master) {
                    $('#KeiyakukinTokuhanShubetsu'+num).append('<option value="'+keiyakukin_tokuhan_master[zo].id+'">'+keiyakukin_tokuhan_master[zo].value+'</option>');
	         }
	      }
      }

      setKeiyakukinShubetsu(0);

//利用料マスタの設置

      function setRiyouryouShubetsu(num) {    
	      if(riyouryou_master.length>0) {
	         for(var zo in riyouryou_master) {
//	            $('.RiyouryouShubetsu').eq(num).append('<option value="'+riyouryou_master[zo].id+'">'+riyouryou_master[zo].value+'</option>');
	            $('#RiyouryouShubetsu'+num).append('<option value="'+riyouryou_master[zo].id+'">'+riyouryou_master[zo].value+'</option>');
                 }
              }
              if(riyouryou_tokuhan_master.length>0) {
                 for(var zo in riyouryou_tokuhan_master) {
                    $('#RiyouryouTokuhanShubetsu'+num).append('<option value="'+riyouryou_tokuhan_master[zo].id+'">'+riyouryou_tokuhan_master[zo].value+'</option>');
	         }
	      }
      }

      setRiyouryouShubetsu(0);

//特販マスタの設置

      function setTokuhanShubetsu(num) {    
	      if(tokuhan_master.length>0) {
	         for(var zo in tokuhan_master) {
//	            $('.TokuhanShubetsu').eq(num).append('<option value="'+tokuhan_master[zo].id+'">'+tokuhan_master[zo].value+'</option>');
	            $('#TokuhanShubetsu'+num).append('<option value="'+tokuhan_master[zo].id+'">'+tokuhan_master[zo].value+'</option>');
	         }
	      }
      }

      setTokuhanShubetsu(0);


