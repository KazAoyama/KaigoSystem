				nyutai="入";
			} else {
				nyutai="退";
			}
			$('#nyutaikyo').append("<tr onclick=\"location.href='#?id="+nyutaikyod[j].id+"'\"><td class=\"table1-value\">"+nyutaikyod[j].kyoshitsu+"</td><td class=\"table1-value\">"+nyutaikyod[j].name+"</td><td class=\"table1-value\">"+nyutai+"</td></tr>");
		}


